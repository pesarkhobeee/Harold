{
   "action": {
      "default_popup": "popup.html"
   },
   "manifest_version" : 3,
   "description": "Harold, the emotional sticky note which encourages you to keep note of your daily discoveries and be more mindful.",
   "icons": {
      "180": "images/icon-180.png",
      "120": "images/icon-120.png",
      "48": "images/icon-48.png"
   },
   "name": "Harold",
   "offline_enabled": true,
   "version": "0.2.0",
   "background": {
      "service_worker": "background.js"
    },
   "permissions": [
      "storage",
      "alarms"
   ]
}
